<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>

    <appSettings>
      <add key="WebServiceHost" value="https://__YOUR_GREENBUTTON_WEBSERVICE_HOST__" />      <!-- Windows Azure example: https://mywebservice.cloudapp.net -->
      <add key="Username" value="__YOUR_GREENBUTTON_ACCOUNT__" />
      <add key="Password" value="__YOUR_GREENBUTTON_PASSWORD__" />
    </appSettings>
    
</configuration>